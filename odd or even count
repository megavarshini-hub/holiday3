#include <stdio.h>

int main() {
    int n, i;
    int a[100];// enough to hold 100 numbers
     int even=0,odd=0;
    scanf("%d", &n); // read number count

    for(i=0;i<n;i++)
    {
      scanf("%d", &a[i]);
      

    if(a[i] % 2 == 0) {
            even++;
        } else {
            odd++;
        }
    }
          

    printf("Even: %d\n", even);
    printf("Odd: %d\n", odd);
     
    return 0;

}
